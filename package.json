{
	"name": "psybook",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite dev",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"paraglide:compile": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
		"paraglide:watch": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide --watch"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.8.1",
		"@lucide/svelte": "^0.515.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@vitest/browser": "^3.2.3",
		"bits-ui": "^2.8.6",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.0",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.0",
		"@stripe/stripe-js": "^7.6.1",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.52.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"lucide-svelte": "^0.525.0",
		"nanoid": "^5.1.5",
		"posthog-js": "^1.257.2",
		"resend": "^4.7.0",
		"shadcn-svelte": "^1.0.6",
		"stripe": "^18.3.0",
		"tailwind-merge": "^3.3.1",
		"twilio": "^5.7.3",
		"zod": "^4.0.5"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
